<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.camnter.newlife">

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.camnter.newlife.robotlegsapplication.MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/zzmbs_2"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.activity.ImageScaleTypesActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.AsyncTaskActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.design.TextInputLayoutActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.RefreshUIActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.design.NavigationViewActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.util.DeviceUtilActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.design.FloatingActionButtonActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.design.SnackbarActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.util.DateUtilActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.EasySlidingTabsActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.AutoAdjustSizeEditTextActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.AutoAdjustSizeTextViewActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.DownloadImageToGalleryActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.SensorManagerActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.design.EasyRecyclerViewActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.design.tablayout.NormalTabLayoutActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.design.tablayout.SetIconTabLayoutActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.design.tablayout.ImageSpanTabLayoutActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.design.tablayout.CustomViewTabLayoutActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.design.CoordinatorActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.singletask.AActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.singletask.VActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.singletask.LActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.singletask.MActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.singletask.SingleTaskActivity"
            android:label="@string/app_label"
            android:launchMode="singleTask"
            android:taskAffinity="" />

        <activity
            android:name="com.camnter.newlife.mvp.MvpActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.SQLiteActivity"
            android:label="@string/app_label" />
        <activity
            android:name="com.camnter.newlife.robotlegs4android.view.activity.Robotlegs4AndroidActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.CustomContentProviderActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.DownloadServiceActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.AIDLActivity"
            android:label="@string/app_label" />
        <activity
            android:name=".view.activity.util.ReflectionUtilActivity"
            android:label="@string/app_label" />

        <service android:name="com.camnter.newlife.service.DownloadService" />

        <!-- action 写上 aidl生成类的所在包 -->
        <service
            android:name=".aidl.PushMessageService"
            android:process=":remote">
            <intent-filter>
                <action android:name="com.camnter.newlife.aidl.IPushMessage" />
            </intent-filter>
        </service>

        <provider
            android:name="com.camnter.newlife.contentprovider.MessageContentProvider"
            android:authorities="com.camnter.content.provider" />

        <meta-data
            android:name="DEBUG"
            android:label="@string/app_label"
            android:value="${DEBUG}" />

    </application>

</manifest>
