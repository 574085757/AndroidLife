<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.camnter.multi.classloader.plugin.host">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_camnter_round"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".MultiClassLoaderApplication">

        <activity android:name=".MultiClassLoaderPluginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name="com.camnter.multi.classloader.plugin.one.PluginOneActivity"
            android:label="PluginOneActivity"/>

        <activity
            android:name="com.camnter.multi.classloader.plugin.two.PluginTwoActivity"
            android:label="PluginTwoActivity"/>

    </application>

</manifest>
